{
  "name": "redux-boilerplate",
  "description": "A starter project for modern React apps with Redux",
  "version": "1.0.0",
  "author": "Sarhan Azizov <sarhan.azizov@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:Jayser/redux-boilerplate.git",
  "bugs": "https://github.com/Jayser/redux-boilerplate/issues",
  "homepage": "https://github.com/Jayser/redux-boilerplate#readme",
  "scripts": {
    "prestart": "npm run clean",
    "start": "cross-env NODE_ENV=development PORT=8000 node dev-server.js",
    "pre-commit": "npm run lint",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "./node_modules/.bin/eslint src",
    "lint:stylelint": "./node_modules/.bin/stylelint src/**/*.scss",
    "typecheck": "flow",
    "clean": "rm -rf dist reports",
    "analyze": "mkdirp reports && webpack --json -profile > reports/webpack-stats.json"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-tcomb": "^0.3.24",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "classnames": "^2.2.5",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "^0.10.0",
    "flow-bin": "^0.38.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.4.0",
    "postcss-loader": "^1.2.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-router-redux": "^4.0.7",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "resolve-url-loader": "^1.6.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.1",
    "stylelint-config-css-modules": "^0.1.0",
    "stylelint-config-standard": "^15.0.1",
    "stylelint-webpack-plugin": "^0.5.1",
    "tcomb": "^3.2.16",
    "url-loader": "^0.5.7",
    "validate-commit-msg": "^2.11.1",
    "webpack": "2.2.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.16.0",
    "webpack-merge": "^2.6.1"
  },
  "dependencies": {
    "autobind-decorator": "^1.3.4",
    "babel-polyfill": "^6.22.0",
    "bootstrap-sass": "^3.3.7",
    "es6-promise": "^4.0.5",
    "express": "^4.14.1",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "config": {
    "commit-msg": "validate-commit-msg"
  },
  "engines": {
    "node": ">=7.4.0"
  }
}
